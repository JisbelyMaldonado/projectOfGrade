}
.menupage {
	background: rgba(255,255,255,.95);
	padding-top: 10px;
	margin-bottom: 10px;
	border-radius: 0px 0px 10px 10px;
/*	box-shadow: 0 0 2px #000;
*/}
.menupage ul {
	float: right;
	margin-top: 20px;
	margin-bottom: 45px;
}
.menupage li {
	list-style: none;
	float: left;
}
.menupage a {
	display: block;
	background: linear-gradient(#1E4EFF,#6E8DFF);
	padding: 20px;
	border-right: 1px solid #6E8DFF;
	color: #00178E;
	font-weight: bold;
	text-decoration: none;
	transition: box-shadow 0.8s;
	transition: color 0.5s;
	font-size: 16px;
	text-shadow: 0 0 3px #fff;
}
.menupage li:first-child a {
	border-radius: 10px 0 0 10px;
}
.menupage li:last-child a {
	border-right: none;
	border-radius: 0 10px 10px 0;
}
.menupage a:hover {
	background: #442BFF;
	color: #fff;
	box-shadow: 0px 0px 1px #000;
}
